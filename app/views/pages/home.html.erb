<div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner carrousel">
    <div class="carousel-item active">
      <img src="https://i0.wp.com/picjumbo.com/wp-content/uploads/private-jet-flying-over-clouds-free-photo.jpg?w=2210&quality=70" class="d-block w-100" alt="...">
      <div class="carousel-caption text1">
        <h3>SkyRentalz</h3>
        <p>Jetting with Joy: SkyRentalz, Your Sky, Your Rules.</p>
        <%= link_to "Book your jet", jets_path, class: "btn rounded-3 btn-primary" %>
      </div>
    </div>
    <div class="carousel-item">
      <img src="https://www.avico.com/wp-content/uploads/2022/02/fad49d1f16efb33ba0fe1582d290c695-1-scaled-e1645453235505.jpg" class="d-block w-100" alt="...">
      <div class="carousel-caption text2">
        <h3>SkyRentalz</h3>
        <p>Tailored Journeys: Explore the Skies in Style.</p>
        <%= link_to "Book your jet", jets_path, class: "btn rounded-3 btn-primary" %>
      </div>
    </div>
    <div class="carousel-item">
      <img src="https://flyingmag.sfo3.digitaloceanspaces.com/flyingma/wp-content/uploads/2022/06/28094407/shutterstock_417878923-scaled.jpg" class="d-block w-100" alt="...">
      <div class="carousel-caption text3 text-end">
        <h3>SkyRentalz</h3>
        <p>Luxury in Every Detail, Sky High with SkyRentalz.</p>
        <%= link_to "Book your jet", jets_path, class: "btn rounded-3 btn-primary" %>
      </div>
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

<div class="feature-section">

</div>

<div class="container grid-home-jets my-5">
  <% @jets.shuffle.take(3).each do |jet| %>
    <div class="card border border-0 image-container" style="width: 20rem;">
      <% jet.photos.each do |photo| %>
        <a href= <%= jet_path(jet) %> >
          <%= cl_image_tag photo.key %>
        </a>
      <% end %>
      <div class="card-body">
        <h5 class="card-title"><%= jet.name %></h5>
        <p class="card-text"><strong>Model:</strong> <%= jet.model %></p>
        <p class="list-group-item" style="color: gray;"><strong>Location:</strong> <%= jet.location %></p>
        <p class="list-group-item" style="color: gray;"><strong>Type:</strong> <%= jet.jetType %></p>
      </div>
    </div>
  <% end %>
</div>

<p>Find me in app/views/pages/home.html.erb</p>
